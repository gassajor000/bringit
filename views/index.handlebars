<html>
<head>
	<title>Bringit!</title>
	<link href="css/bootstrap.css" rel="stylesheet">
	<link href="css/bootstrap.min.css" rel="stylesheet">
	<link href="css/bootstrap-theme.css" rel="stylesheet">
	<link href="css/bootstrap-thime.min.css" rel="stylesheet">
	<link href="css/index.css" rel="stylesheet">
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
</head>

<body>
	
	<h1 class="text-center">Bringit!</h1>
	<h3 class="text-center">My Lists</h3>
	{{#each events}}
		<div class="event"> 
			<a href="/event">
			<h3>{{title}}</h3><span><h3>{{date}}</h3></span>
			<p class="text-left">
				{{#each guests}}{{getUserFullName this}}, {{/each}}
			</p>
			</a>
			
		</div>
		<hr>
	{{/each}}
	<div class="text-center"><a alt="add" href="/"><i class="material-icons">add</i></a></div>
</body>
</html>
